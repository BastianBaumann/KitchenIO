<Window x:Class="Frontend.Windows.TakeItemWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frontend.Windows"
        mc:Ignorable="d"
        Title="TakeItemWindow" Height="77" Width="134">
    <Grid Margin="0,0,0,-6">
        <TextBox Name="AmountTextBox" Text="Amount.." Margin="0,0,0,47"/>
        <Button x:Name="SendTakeItem" Content="OK" Margin="10,25,30,15" Click="CheckAmount"/>
    </Grid>
</Window>
