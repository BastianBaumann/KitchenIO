<Window x:Class="Frontend.Windows.CheckItems"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frontend.Windows"
        mc:Ignorable="d"
        Title="CheckItems" Height="250" Width="450">
    <Grid>
        <DataGrid x:Name="CheckFoodGrid" Margin="0,0,50,0"/>
        <Button x:Name="Close" Content="Close" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,20,10,0" Click="CloseWindow"/>
    </Grid>
</Window>
